{
  "tasks": {
    "dev": "deno run --watch --deny-env --allow-read main.tsx",
    "test": "deno test --watch --allow-read test/",
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/fs": "jsr:@std/fs@^1.0.8",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/testing": "jsr:@std/testing@^1.0.8",
    "preact": "npm:preact@^10.25.3",
    "preact-render-to-string": "npm:preact-render-to-string@^6.5.12",
    "@types/react": "npm:@types/react@^19.0.2",
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxImportSourceTypes": "@types/react",
    "noImplicitAny": false,
  },
}
